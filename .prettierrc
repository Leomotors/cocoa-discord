{
    "bracketSpacing": true,
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": false,
    "semi": true,
    "printWidth": 80,
    "importOrder": [
        "^\\.\\/stub",
        "^[a-zA-Z]",
        "^[.][.]",
        "^[.]"
    ],
    "importOrderCaseInsensitive": true,
    "importOrderSeparation": true,
    "importOrderParserPlugins": [
        "typescript",
        "decorators-legacy"
    ]
}