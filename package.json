{
  "name": "cocoa-discord-utils",
  "version": "1.4.0-dev",
  "description": "Yet another Discord Bot Utility npm package",
  "main": "dist/main/index.js",
  "repository": "https://github.com/Leomotors/cocoa-discord-utils",
  "homepage": "https://github.com/Leomotors/cocoa-discord-utils",
  "author": "Leomotors",
  "license": "MIT",
  "scripts": {
    "build-inner": "tsc && tsc -p tsconfig.bruh.json",
    "build": "yarn lm:config && yarn build-inner && rimraf \"dist/cli/**/*.d.ts\"",
    "clean": "rimraf dist/*",
    "cb": "yarn clean && yarn build",
    "lm:config": "lm-config src/meta/cocoa_meta.g.ts",
    "prepack": "yarn cb && yarn lint && yarn test",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "eslint src && prettier --check \"{src,tests}/**/*.ts\"",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"resolveJsonModule\":true}' mocha -r ts-node/register --exit tests/index.spec.ts",
    "ncu": "ncu -u \"/^(?!.*chalk)(?!.*globby).*$/\"",
    "pre-doc": "ts-node predoc.script.ts",
    "docs": "typedoc",
    "post-doc": "ts-node postdoc.script.ts",
    "full-doc": "yarn lm:config && yarn pre-doc && yarn docs && yarn post-doc"
  },
  "keywords": [
    "discord-bot",
    "slash-commands"
  ],
  "devDependencies": {
    "@leomotors/scripts": "^3.0.7",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/chai": "^4.3.0",
    "@types/marked": "^4.0.3",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.22",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4.3.6",
    "discord-api-types": "^0.29.0",
    "eslint": "^8.11.0",
    "globby": "11.1.0",
    "highlight.js": "^11.5.0",
    "marked": "^4.0.12",
    "mocha": "^9.2.2",
    "npm-check-updates": "^12.5.4",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "chalk": "4.1.2",
    "commander": "^9.1.0",
    "discord.js": "^13.6.0"
  },
  "exports": {
    ".": "./dist/main/index.js",
    "./message": "./dist/message/index.js",
    "./message/class": "./dist/message/class/index.js",
    "./meta": "./dist/meta/index.js",
    "./slash": "./dist/slash/index.js",
    "./slash/class": "./dist/slash/class/index.js",
    "./template": "./dist/template/index.js",
    "./internal/base": "./dist/base/index.js",
    "./internal/SlashSync": "./dist/slash/SlashSync.js"
  },
  "typings": "./dist/main/index.d.ts",
  "typesVersions": {
    "*": {
      "message": [
        "./dist/message/index.d.ts"
      ],
      "message/class": [
        "./dist/message/class/index.d.ts"
      ],
      "meta": [
        "./dist/meta/index.d.ts"
      ],
      "slash": [
        "./dist/slash/index.d.ts"
      ],
      "slash/class": [
        "./dist/slash/class/index.d.ts"
      ],
      "template": [
        "./dist/template/index.d.ts"
      ],
      "internal/base": [
        "./dist/base/index.d.ts"
      ],
      "internal/SlashSync": [
        "./dist/slash/SlashSync.d.ts"
      ]
    }
  },
  "bin": {
    "cocoadu": "dist/cli/index.js"
  }
}